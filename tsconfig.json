{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitReturns": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "es2020"
	},
	"include": [
		"src/**/*"
	]
}